{
  "name": "cmdo-server",
  "version": "0.0.0",
  "description": "Node.js server framework",
  "repository": "https://github.com/cmdo/toolkit.git",
  "author": "Christoffer RÃ¸dvik <dev@kodemon.net>",
  "license": "MIT",
  "keywords": [
    "server",
    "http",
    "ws"
  ],
  "main": "./cjs/index.js",
  "module": "./esm/index.js",
  "types": "./types/index.d.ts",
  "jest": {
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig-cjs.json"
      }
    },
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.ts?$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ]
  },
  "scripts": {
    "prepublishOnly": "npm run lint && npm run build",
    "build": "npm run clean && npm run build:esm && npm run build:cjs",
    "build:esm": "./node_modules/.bin/tsc --project tsconfig.json && ./node_modules/.bin/cmdo output esm -f esm",
    "build:cjs": "./node_modules/.bin/tsc --project tsconfig-cjs.json",
    "clean": "rm -rf ./cjs ./esm ./types",
    "lint": "./node_modules/.bin/eslint 'src/**/*.ts' --fix",
    "test": "./node_modules/.bin/jest --runInBand --verbose"
  },
  "dependencies": {
    "ioredis": "4.28.2",
    "path-to-regexp": "6.2.0",
    "ws": "8.4.0"
  },
  "devDependencies": {
    "@types/ioredis": "4.28.5",
    "@types/jest": "27.0.3",
    "@types/node": "17.0.2",
    "@types/ws": "8.2.2",
    "@typescript-eslint/eslint-plugin": "5.8.0",
    "@typescript-eslint/parser": "5.8.0",
    "cmdo-cli": "0.2.5",
    "eslint": "8.5.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "prettier": "^2.5.1",
    "ts-jest": "27.1.2",
    "typescript": "4.5.4"
  }
}
